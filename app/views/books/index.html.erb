<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">titulo</th>
            <th scope="col">autor</th>
            <th scope="col">estado</th>
        </tr>
    </thead>
    <tbody>
        <% @books.each do |book|%>
        <tr>
            <th scope="row"><%= book.id %> </th>
            <td><%= book.title %> </td>
            <td><%= book.author %> </td>
            <td><%= book.status %> </td>
        </tr>

        <%end%>
        
        
    </tbody>
</table>
